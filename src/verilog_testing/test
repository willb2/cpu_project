#! /usr/local/bin/vvp
:ivl_version "0.9.5 " "(v0_9_5)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x7ff402404910 .scope module, "counter_tb" "counter_tb" 2 26;
 .timescale 0 0;
v0x7ff40241ad90_0 .var "clk", 0 0;
v0x7ff40241ae40_0 .net "cout", 3 0, v0x7ff40241abf0_0; 1 drivers
v0x7ff40241aee0_0 .var "en", 0 0;
v0x7ff40241afa0_0 .var "rst", 0 0;
S_0x7ff4024049f0 .scope module, "t_counter" "counter" 2 48, 2 1, S_0x7ff402404910;
 .timescale 0 0;
v0x7ff402404850_0 .net "clk", 0 0, v0x7ff40241ad90_0; 1 drivers
v0x7ff40241abf0_0 .var "cout", 3 0;
v0x7ff40241ac80_0 .net "en", 0 0, v0x7ff40241aee0_0; 1 drivers
v0x7ff40241ad00_0 .net "rst", 0 0, v0x7ff40241afa0_0; 1 drivers
E_0x7ff4024061c0 .event posedge, v0x7ff402404850_0;
S_0x7ff40240ad40 .scope begin, "COUNTER" "COUNTER" 2 14, 2 14, S_0x7ff4024049f0;
 .timescale 0 0;
    .scope S_0x7ff4024049f0;
T_0 ;
    %wait E_0x7ff4024061c0;
    %fork t_1, S_0x7ff40240ad40;
    %jmp t_0;
    .scope S_0x7ff40240ad40;
t_1 ;
    %load/v 8, v0x7ff40241ad00_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 4, 0;
    %assign/v0 v0x7ff40241abf0_0, 1, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x7ff40241ac80_0, 1;
    %jmp/0xz  T_0.2, 8;
    %load/v 8, v0x7ff40241abf0_0, 4;
    %mov 12, 0, 28;
    %addi 8, 1, 32;
    %ix/load 0, 4, 0;
    %assign/v0 v0x7ff40241abf0_0, 1, 8;
T_0.2 ;
T_0.1 ;
    %end;
    .scope S_0x7ff4024049f0;
t_0 %join;
    %jmp T_0;
    .thread T_0;
    .scope S_0x7ff402404910;
T_1 ;
    %vpi_call 2 32 "$display", "time\011 clk reset enable counter";
    %vpi_call 2 33 "$monitor", "%g\011 %b   %b     %b      %b", $time, v0x7ff40241ad90_0, v0x7ff40241afa0_0, v0x7ff40241aee0_0, v0x7ff40241ae40_0;
    %set/v v0x7ff40241ad90_0, 1, 1;
    %set/v v0x7ff40241afa0_0, 0, 1;
    %set/v v0x7ff40241aee0_0, 0, 1;
    %delay 5, 0;
    %set/v v0x7ff40241afa0_0, 1, 1;
    %delay 10, 0;
    %set/v v0x7ff40241afa0_0, 0, 1;
    %delay 10, 0;
    %set/v v0x7ff40241aee0_0, 1, 1;
    %delay 100, 0;
    %set/v v0x7ff40241aee0_0, 0, 1;
    %delay 5, 0;
    %vpi_call 2 41 "$finish";
    %end;
    .thread T_1;
    .scope S_0x7ff402404910;
T_2 ;
    %delay 5, 0;
    %load/v 8, v0x7ff40241ad90_0, 1;
    %inv 8, 1;
    %set/v v0x7ff40241ad90_0, 8, 1;
    %jmp T_2;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "tb_practice.v";
